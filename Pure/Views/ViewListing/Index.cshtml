        // Creating a map with DOM element
        map = new google.maps.Map(mapElement, mapOptions);
        map2 = new google.maps.Map(mapElement2, mapOptions);


        marker = new google.maps.Marker({
            position: new google.maps.LatLng(44.4268, 26.1025),
        });

        marker.setMap(map);

    };

    <script>
    jQuery(document).ready(function ($) {
        initMap();
    });

    var map, map2;
    var marker;
    window.initMap = function () {
        var mapOptions = {
            center: new google.maps.LatLng(44.4268, 26.1025),
            zoom: 13,
            mapTypeIds: google.maps.MapTypeId.ROADMAP,
            mapTypeControl: false,
            panControl: false,
            scaleControl: false,
            streetViewControl: false,
            zoomControl: true,
            scrollwheel: false,
            gestureHandling: 'cooperative',
            styles: [
                {
                    "featureType": "poi",
                    "stylers": [
                        {"visibility": "off"}
                    ]
                },
                {
                    "featureType": "transit",
                    "elementType": "labels.icon",
                    "stylers": [
                        {"visibility": "off"}
                    ]
                }
            ]
        };
        var mapElement = document.getElementById('map-content');
        var mapElement2 = document.getElementById('map-content2');

        // Creating a map with DOM element
        map = new google.maps.Map(mapElement, mapOptions);
        map2 = new google.maps.Map(mapElement2, mapOptions);


        marker = new google.maps.Marker({
            position: new google.maps.LatLng(44.4268, 26.1025),
        });

        marker.setMap(map);

    };
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD9CPslB847Y19IvO3n00JeJo5cIzpjU5U&amp;callback"></script>

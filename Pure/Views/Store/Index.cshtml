@model Homepage

    @{
        ViewBag.Tittle = "Index";
    }



<!DOCTYPE html>
<html lang="en" dir="ltr">


<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
    <meta name="keywords" content="Premium HTML5 Template" />
    <meta name="description" content="ThePure - Responsive Bootstrap Classified Ads HTML Template" />
    <meta name="author" content="CodinBit.com" />
    <title>ThePure - Responsive Bootstrap Classified Ads HTML Template</title>


    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/font-awesome/css/all.css">
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/flags2/flag-css.css">
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/select2/dist/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Public/vendor/ekko-lightbox/ekko-lightbox.css" />

    <link rel="stylesheet" type="text/css" href="~/Public/css/style.css">
</head>
<body>
    <div class="page-loader loading">
        <div class="loader">
            <i class="fas fa-spinner fa-spin" aria-hidden="true"></i>
        </div>
    </div>
    <div class="overlay-nav"></div>


    @{
        Html.RenderPartial("~/Views/Shared/_Header.cshtml");
    }



<div class="content">
    <section class="bkg-light store">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="view-store">
                        <div class="infos">
                            <div class="row">
                                <div class="col-12 col-lg-8 col-xl-8">
                                    <div class="row">
                                        <div class="col-12">
                                            <div class="gallery">
                                                <div class="slider-single animation-slide height-auto control-nav">
                                                    <div class="slider-item">
                                                        <div class="image" style="background-image: url('Public/images/stores/2000X1125_store-slider-1.jpg');">
                                                            <img src="~/Public/images/stores/2000X1125_store-slider-1.jpg" alt="" />
                                                        </div>
                                                    </div>
                                                    <div class="slider-item">
                                                        <div class="image" style="background-image: url('Public/images/stores/2000X1125_store-slider-2.jpg');">
                                                            <img src="~/Public/images/stores/2000X1125_store-slider-2.jpg" alt="" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 d-none d-lg-block">
                                            <div class="search-block">
                                                <div class="row">
                                                    <div class="col-12 col-lg-10">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                <div class="row">
                                                                    <div class="col-12 col-md-4">
                                                                        <div class="form-group icon">
                                                                            <i class="fa fa-search" aria-hidden="true"></i>
                                                                            <input type="text" class="form-control" placeholder="Search for anything" value="">
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-12 col-md-4">
                                                                        <div class="form-group icon">
                                                                            <i class="fa fa-map-marker-alt" aria-hidden="true"></i>
                                                                            <input type="text" class="form-control" placeholder="Location" value="">
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-12 col-md-4">
                                                                        <div class="form-group icon">
                                                                            <i class="fa fa-bars" aria-hidden="true"></i>
                                                                            <select>
                                                                                <option>Choose Category</option>
                                                                                @foreach (var item in Model.ChooseCategories)
                                                                                {
                                                                                    <option value="@item.Name">@item.Name</option>
                                                                                }


                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-lg-2">
                                                        <div class="form-group">
                                                            <button class="btn btn-primary" type="button">Search</button>
                                                        </div>
                                                        <div class="form-group text-center">
                                                            <ul class="display-opts" data-listing-for="listing-1">
                                                                <li><a href="#" class="btn-grid"><i class="fas fa-th"></i></a></li>
                                                                <li><a href="#" class="btn-list"><i class="fas fa-th-list"></i></a></li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-lg-4 col-xl-4">
                                    <div class="actions-block">
                                        <ul class="stack">
                                            <li>
                                                <span class="user-details-wrapper">
                                                    <span class="user-details">
                                                        <span class="user-img">
                                                            <span class="avatar text-center">
                                                                <img src="~/Public/images/stores/500X500_jaguar.jpg">
                                                            </span>
                                                        </span>
                                                        <span class="user-name">
                                                            <span class="name">
                                                                Jaguar Auto
                                                                <span class="verified"><i class="far fa-check-circle"></i> Verified</span>
                                                            </span>
                                                        </span>
                                                    </span>
                                                </span>
                                            </li>
                                            <li>
                                                <ul class="nav nav-tabs">
                                                    <li class="nav-item">
                                                        <a class="nav-link active" data-toggle="tab" href="#store-products">Our Store</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" data-toggle="tab" href="#store-about">About Us</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" data-toggle="tab" href="#store-terms">Terms & Conditions</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" data-toggle="tab" href="#store-reviews">Reviews</a>
                                                    </li>
                                                </ul>
                                            </li>
                                            <li>
                                                <span class="joined">
                                                    <strong>Joined:</strong> 1 years ago
                                                </span>
                                                <span class="location">
                                                    <strong>Address:</strong> California, United States
                                                </span>
                                                <div class="rating rated">
                                                    <span>★</span><span>★</span><span>★</span><span>★</span><span>★</span>
                                                </div>
                                                <span class="reviews">(158 reviews)</span>
                                            </li>
                                            <li>
                                                <ul class="links">
                                                    <li><a href="#" target="_blank"><i class="fa fa-envelope" aria-hidden="true"></i> Show email</a></li>
                                                    <li><a href="#" target="_blank"><i class="fa fa-phone" aria-hidden="true"></i> Show number</a></li>
                                                    <li><a href="#" target="_blank"><i class="fa fa-map-marker-alt" aria-hidden="true"></i> Show location</a></li>
                                                </ul>
                                                <ul class="social">
                                                    <li><a href="#" target="_blank"><i class="fab fa-facebook" aria-hidden="true"></i></a></li>
                                                    <li><a href="#" target="_blank"><i class="fab fa-twitter" aria-hidden="true"></i></a></li>
                                                    <li><a href="#" target="_blank"><i class="fab fa-linkedin" aria-hidden="true"></i></a></li>
                                                    <li><a href="#" target="_blank"><i class="fab fa-google-plus-g" aria-hidden="true"></i></a></li>
                                                    <li><a href="#" target="_blank"><i class="fab fa-instagram" aria-hidden="true"></i></a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @{
        Html.RenderPartial("~/Views/Shared/_MyProduct.cshtml");
    }
</div>




   
 

    <script src="~/Public/vendor/jquery/jquery.min.js"></script>

    <script src="~/Public/vendor/popper.min.js"></script>
    <script src="~/Public/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Public/vendor/slick/slick.js"></script>
    <script src="~/Public/vendor/select2/dist/js/select2.min.js"></script>
    <script src="~/Public/vendor/ekko-lightbox/ekko-lightbox.js"></script>
    <script src="../../www.google.com/recaptcha/api.js" async defer></script>
    <script src="~/Public/js/main.js"></script>
</body>


</html>

